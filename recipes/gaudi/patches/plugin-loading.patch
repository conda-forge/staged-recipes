+#####################################################################################
+import ctypes
+import os
+import sys
+from ctypes.util import find_library
+from functools import lru_cache
+
+
+@lru_cache(maxsize=1)
+def _get_default_plugin_path():
+    """Get default plugin path from libGaudiPluginService using dladdr."""
+    import logging
+
+    log = logging.getLogger(__name__)
+
+    # Check for explicit library path from build environment
+    lib_path = os.environ.get("GAUDI_PLUGINSVC_LIBRARY")
+    if not lib_path:
+        # Fallback: use find_library (works when properly installed)
+        lib_path = find_library("GaudiPluginService")
+
+    if not lib_path:
+        log.debug("Could not find GaudiPluginService library")
+        return ""
+
+    try:
+        lib = ctypes.CDLL(lib_path)
+        lib.cgaudi_pluginsvc_get_default_search_path.restype = ctypes.c_char_p
+        result = lib.cgaudi_pluginsvc_get_default_search_path()
+        path = result.decode() if result else ""
+        log.debug("Default plugin path: %s", path)
+        return path
+    except OSError as e:
+        log.debug("Failed to get default plugin path: %s", e)
+        return ""
+
+
+# For backwards compatibility
+GAUDI_DEFAULT_PLUGIN_PATH = _get_default_plugin_path()
+
+
+def get_plugin_search_paths():
+    """Get plugin search paths: default path first, then from environment variables.
+
+    On macOS: uses GAUDI_PLUGIN_PATH only (LD_LIBRARY_PATH is ignored by SIP).
+    On Linux: uses GAUDI_PLUGIN_PATH and LD_LIBRARY_PATH.
+
+    This matches the C++ behavior in PluginServiceV2.cpp.
+    """
+    if sys.platform == "darwin":
+        env_vars = ["GAUDI_PLUGIN_PATH"]
+    else:
+        env_vars = ["GAUDI_PLUGIN_PATH", "LD_LIBRARY_PATH"]
+
+    paths = []
+    if GAUDI_DEFAULT_PLUGIN_PATH:
+        paths.append(GAUDI_DEFAULT_PLUGIN_PATH)
+
+    for var in env_vars:
+        paths.extend(p for p in os.environ.get(var, "").split(os.pathsep) if p)
+
+    return paths
+
+
+def gaudi_kernel_library_path():
+    """Get the path to the GaudiKernel library."""
+    if sys.platform == "darwin":
+        lib_name = "libGaudiKernel.dylib"
+    else:
+        lib_name = "libGaudiKernel.so"
+
+    paths = get_plugin_search_paths()
+    for path in paths:
+        lib_path = os.path.join(path, lib_name)
+        if os.path.isfile(lib_path):
+            return lib_path
+
+    if lib_path := find_library("GaudiKernel"):
+        return lib_path
+
+    msg = f"{lib_name} not found. "
+    if paths:
+        msg += "Searched paths: " + ":".join(paths)
+    else:
+        msg += "GAUDI_PLUGIN_PATH not set"
+    raise RuntimeError(msg)
